{
  "name": "recipe-hub",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/recipe-hub/",
  "projectType": "application",
  "tags": ["type:app"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nuxt build",
        "cwd": "{workspaceRoot}/apps/recipe-hub"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nuxt dev --port 3002",
        "cwd": "{workspaceRoot}/apps/recipe-hub"
      }
    }
  }
}
